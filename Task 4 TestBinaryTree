public class TestBinaryTree {
    public static void main(String[] args) {
        BinaryTree tree = new BinaryTree();
        System.out.println("Empty\t: "+tree.isEmpty());

        tree.addNode(12);
        tree.addNode(40);
        tree.addNode(50);

        System.out.println("Empty\t: "+tree.isEmpty());
        Node node=new Node();
        node=tree.getRoot();

        System.out.println("Level Order\t: "+tree.printLevelOrder(node));
    }//end of main class
}//end of class
